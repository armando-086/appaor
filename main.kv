<LeftLabel@Label>:
    text_size: self.size
    halign: 'left'

<MessagePopup>:
    BoxLayout:
        orientation: 'vertical'
        Label:
            id: message
            size_hint: 1,0.8
            text: ''
        Button:
            size_hint: 1,0.2
            text: 'Regresar'
            on_press: root.dismiss()

<InsertData_SM>:
    data: ''
    data_id: ''
    MDBoxLayout:
        orientation: "vertical"
        padding: 10
        spacing:10
        size_hint: 1,1
        MDBoxLayout:
            size_hint: 1,.8
            orientation:"horizontal"
            padding: "10dp"
            MDCard:
                radius:"10dp"
                padding:"10dp"
                line_color: "green"
                MDLabel:
                    text:"EJERCICIO MUSCULAR"
                    font_size:20
                    text_size:self.size
                    valign:"middle"
                    halign:"center"
                    color:"green"
                    bold: True
         #*******FIN ***TITULO*****************************************
        #*********************AGREGAR EJERCICIO *******************
        MDBoxLayout:
            size_hint:1,.35
            Spinner:
                id: sm1
                text: 'SELECCIONE_MUSCULO'
                values: ["PECHO","ESPALDA","ABDOMEN", "HOMBRO", "BICEP","TRICEP","ANTEBRAZO","CUADRICEP","ISQUIO","ABDUCTORE","PANTORRILLA","GLUTEO"]
                sync_height: True
                on_text: root.select_sesion_m()
                #SpinnerOption:
                #option_cls: Factory.sp
                #background_color:"black"#1,1,1
                # color:"black"
