<MainWid>:
    canvas:
        Color:
            #rgb:0,.5,.5,1
            rgb:255,255,255,
        Rectangle:
            pos: self.pos
            size: self.size

#************************************************************************************************
#****************************PRIMERA VENTANA**MENU PRINCIPAL*************************************
#*********************************************************************************
<StartWid>:
    md_bg_color:255,255,255,255
    MDBoxLayout:
        orientation: "vertical"
        MDBoxLayout:
            size_hint: 1,.2
            orientation:"horizontal"
            padding: "10dp"
            MDCard:
                radius:"10dp"
                padding:"10dp"
                line_color: "green"
                MDLabel:
                    text:"CONTROL DE ENTRENAMIENTO"
                    font_size:20
                    text_size:self.size
                    valign:"middle"
                    halign:"center"
                    color:"green"
                    bold: True
                    
                
        #*************FIN---TITULO*************************
        #*************INICIO---MENU*************************
        MDGridLayout:
            cols:2
            size_hint:1,.8
            padding:["10dp","10dp","10dp","10dp"]
            spacing:"10dp" 
            MDCard:
                radius:"10dp"
                padding:"10dp"
                line_color: "green"
                ripple_behavior:True
                on_press: root.create_database_sesion_ejercicio()
                MDBoxLayout:
                    orientation: "vertical"
                    Image:
                        source:"iniciar_entrenamiento_1.jfif"
                        allow_stretch: True
                        keep_ratio: False
                    MDLabel:
                        text:"INICIAR ENTRENAMIENTO"
                        font_size:16
                        text_size:self.size
                        valign:"middle"
                        halign:"center"
                        color:"green"
                        bold: True
                        size_hint: 1,.3
            MDCard:
                radius:"10dp"
                padding:"10dp"
                line_color: "green"
                ripple_behavior:True
                on_press: root.create_database_sesion_muscular()
                MDBoxLayout:
                    orientation: "vertical"
                    Image:
                        source:"sesion_muscular.png"
                        allow_stretch: True
                        keep_ratio: False
                    MDLabel:
                        text:"SESION MUSCULAR"
                        font_size:16
                        text_size:self.size
                        valign:"middle"
                        halign:"center"
                        color:"green"
                        bold: True
                        size_hint: 1,.3
            MDCard:
                radius:"10dp"
                padding:"10dp"
                line_color: "green"
                ripple_behavior:True
                on_press: root.nivel_intensidad()
                MDBoxLayout:
                    orientation: "vertical"
                    Image:
                        source:"nivel_intensidad.png"
                        allow_stretch: True
                        keep_ratio: False
                    MDLabel:
                        text:"NIVEL DE INTENSIDAD"
                        font_size:16
                        text_size:self.size
                        valign:"middle"
                        halign:"center"
                        color:"green"
                        bold: True
                        size_hint: 1,.3
            MDCard:
                radius:"10dp"
                padding:"10dp"
                line_color: "green"
                ripple_behavior:True
                on_press: root.nivel_estress()
                MDBoxLayout:
                    orientation: "vertical"
                    Image:
                        source:"nivel_fatiga3.jfif"
                        allow_stretch: True
                        keep_ratio: False
                    MDLabel:
                        text:"NIVEL DE ESTRESS"
                        font_size:16
                        text_size:self.size
                        valign:"middle"
                        halign:"center"
                        color:"green"
                        bold: True
                        size_hint: 1,.3

#*******************************************************************************************************
#*********************FIN**PRIMERA VENTANA**MENU PRINCIPAL***********************************************
#********************************************************************************************************

#*******************************************************************************************************
#*************************SEGUNDA VENTANA****VISUALIZAR SESION MUSCULAR*********************************
#********************************************************************************************************
#************************DATABASE GENERAL******************************************************
#***********************CREAR TARJETA IMPRESA INFORMACION 
<DataWid_SMG>:
    data: ''
    data_id: ''
    canvas:
        Color:
            rgba: 0,0,1,.5
        Rectangle:
            size: self.size
            pos: self.pos
    MDLabel:
        size_hint_x: 0.9
        size_font: self.width*0.4
        text: root.data
        font_size:15
    MDFlatButton:
        size_hint_x: 0.06
        text: 'Edit'
        theme_text_color: "Custom"
        text_color:"white"
        font_size:18
        adaptive_size: True
        md_bg_color:0,.5,.5,.6
        on_press: root.update_data_SMG(root.data_id)

<Button_ver_SMG>:#BOTON AGREGAR
    font_size: 15#self.height*0.5
    text: 'VER'
    font_size:16
    size_hint:1,1
    adaptive_size: True
    on_press: self.visualizar_SMG()

<Button_atras_SMG>:#BOTON AGREGAR
    font_size: 15#self.height*0.5
    text: 'ATRAS'
    font_size:16
    size_hint:1,1
    adaptive_size: True
    pos_hint: {"center_x": .5, "center_y": .5}
    on_press: self.atras_insertdata_SM()

<Button_inicio_SMG>:#BOTON INICIO
    font_size: 15#self.height*0.5
    text: 'INICIO'
    font_size:16
    size_hint:1,1
    adaptive_size: True
    pos_hint: {"center_x": .5, "center_y": .5}
    on_press: self.back_to_StartWid()
    
<DataBase_SMG>:
    MDBoxLayout:
        orientation: "vertical"
        padding: 10
        spacing:10
        size_hint: 1,1
        MDBoxLayout:
            size_hint: 1,.2
            orientation:"horizontal"
            padding: "10dp"
            MDCard:
                radius:"10dp"
                padding:"10dp"
                line_color: "green"
                MDLabel:
                    text:"VISUALIZAR SESION MUSCULAR"
                    font_size:15
                    text_size:self.size
                    valign:"middle"
                    halign:"center"
                    color:"green"
                    bold: True
         #*******FIN ***TITULO*****************************************
        MDBoxLayout:
            size_hint:1,.08
            Spinner:
                id: sm2
                text: 'PECHO'
                values: ["PECHO","ESPALDA","ABDOMEN", "HOMBRO", "BICEP","TRICEP","ANTEBRAZO","CUADRICEP","ISQUIO","ABDUCTORE","PANTORRILLA","GLUTEO"]
                sync_height: True
                on_text: root.select_sesion_m2()
        ScrollView:
            MDGridLayout:
                id: agregar_smg
                padding: [10,10,10,10]
                spacing: 5
                size_hint_y: None
                cols: 1
                row_default_height: root.height*0.1
                height: self.minimum_height
        #**************BOTONES*************
        MDBoxLayout:
            id: inicio_smg
            padding: 5
            spacing:5
            orientation: 'horizontal'
            size_hint:1,.1
            canvas:
                Color:
                    rgb: 1,1,1
                Rectangle:
                    pos: self.pos
                    size: self.size
