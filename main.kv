            size_hint:1,.35
            Spinner:
                id: ti_articulacion
                text: 'ARTICULACION'
                values: ["MONO","MULTI"]
                on_text: root.select_articular()
                sync_height: True
        MDBoxLayout:
            size_hint:1,.35
            Spinner:
                id: ti_modo
                text: 'MODO'
                values: ["PESO LIBRE","MAQUINA CON PESO","MAQUINA CON POLEA"]
                on_text: root.select_modo()
                sync_height: True
        MDBoxLayout:
            size_hint:1,.4
        MDBoxLayout:
            size_hint:1,.2
            orientation: 'horizontal'
            spacing:5
            MDTextField:
                mode: "rectangle"
                id: ti_abrv
                font_size:18
                text_size:self.size
                valign:"middle"
                halign:"center"
                multiline: False
                hint_text: 'ABRV'
                max_text_length: 5
                size_hint:.3,4.3
                on_text: root.select_abrv()
            MDTextField:
                mode: "rectangle"
                id: ti_ejercicio
                font_size:18
                text_size:self.size
                valign:"middle"
                halign:"center"
                multiline: True
                hint_text: 'EJERCICIO'
                max_text_length: 34
                on_text: root.select_ejercicio()
        MDBoxLayout:
            size_hint:1,.15
        #**************MUSCULOS SECUNDARIO*************************
        MDBoxLayout:
            size_hint:1,.8
            orientation: 'horizontal'
            spacing:2
            MDTextField:
                mode: "rectangle"
                id: ti_pec
                font_size:18
                text_size:self.size
                valign:"middle"
                halign:"center"
                multiline: False
                hint_text: 'PEC_%'
            MDTextField:
                mode: "rectangle"
                id: ti_esp
                font_size:18
                text_size:self.size
                valign:"middle"
                halign:"center"
                multiline: False
                hint_text: 'ESP_%'
            MDTextField:
                mode: "rectangle"
                id: ti_abd
                font_size:18
                text_size:self.size
                valign:"middle"
                halign:"center"
                multiline: False
                hint_text: 'ABD_%'
            MDTextField:
                mode: "rectangle"
                id: ti_hom
                font_size:18
                text_size:self.size
                valign:"middle"
                halign:"center"
                multiline: False
                hint_text: 'HOM_%'
        MDBoxLayout:
            size_hint:1,.8
            orientation: 'horizontal'
            spacing:2
            MDTextField:
                mode: "rectangle"
                id: ti_bic
                font_size:18
                text_size:self.size
                valign:"middle"
                halign:"center"
                multiline: False
                hint_text: 'BIC_%'
            MDTextField:
                mode: "rectangle"
                id: ti_tri
                font_size:18
                text_size:self.size
                valign:"middle"
                halign:"center"
                multiline: False
                hint_text: 'TRI_%'
            MDTextField:
                mode: "rectangle"
                id: ti_ant
                font_size:18
                text_size:self.size
                valign:"middle"
                halign:"center"
                multiline: False
                hint_text: 'ANT_%'
            MDTextField:
                mode: "rectangle"
                id: ti_cua
                font_size:18
                text_size:self.size
                valign:"middle"
                halign:"center"
                multiline: False
                hint_text: 'CUA__%'
        MDBoxLayout:
            size_hint:1,.8
            orientation: 'horizontal'
            spacing:2
            MDTextField:
                mode: "rectangle"
                id: ti_isq
                font_size:18
                text_size:self.size
                valign:"middle"
                halign:"center"
                multiline: False
                hint_text: 'ISQ_%'
            MDTextField:
                mode: "rectangle"
                id: ti_abdu
                font_size:18
                text_size:self.size
                valign:"middle"
                halign:"center"
                multiline: False
                hint_text: 'ABDU_%'
            MDTextField:
                mode: "rectangle"
                id: ti_pan
                font_size:18
                text_size:self.size
                valign:"middle"
                halign:"center"
                multiline: False
                hint_text: 'PAN_%'
            MDTextField:
                mode: "rectangle"
                id: ti_glu
                font_size:18
                text_size:self.size
                valign:"middle"
                halign:"center"
                multiline: False
                hint_text: 'GLU__%'
        #*******************MUSCULOS SECUNDARIO****************************
        #*********************DATOS EJERCICIO MUSCULAR***** 
        MDBoxLayout:
            size_hint:1,.3
        #*********************CALCULAR 1RM***** 
        MDBoxLayout:
            spacing:2
            size_hint:1,.5
            MDTextField:
                mode: "rectangle"
                id: ti_1rm
                multiline: True
                hint_text: '1RM'
                on_text: root.select_1rm()
            MDTextField:
                mode: "rectangle"
                id: ti_peso1
                multiline: True
                hint_text: 'PESO'
                imput_filter:"float"
            MDTextField:
                mode: "rectangle"
                id: ti_no_rep
                multiline: True
                hint_text: 'NO.REP' 
                imput_filter:"float"  
            MDRaisedButton:
                text:'CALCULAR'
                font_size:16
                adaptive_size: True
                pos_hint: {"center_x": .5, "center_y": .5}
                on_press: root.calcular_RM()
        #************************CALCULAR 1RM************************  
        #************************BOTONES************************ 
        MDBoxLayout:
            size_hint:1,.1
        MDBoxLayout:
            spacing:2
            size_hint:1,.4
            MDRaisedButton:
                text:'ATRAS'
                font_size:16
                size_hint:1,1
                adaptive_size: True
                pos_hint: {"center_x": .5, "center_y": .5}
                on_press: root.back_to_StartWid()
            MDRaisedButton:
                text:'AGREGAR'
                font_size:16
                size_hint:1,1
                adaptive_size: True
                pos_hint: {"center_x": .5, "center_y": .5}
                on_press: root.insert_data_SM()
            MDRaisedButton:
                text:'EDITAR'
                font_size:16
                size_hint:1,1
                adaptive_size: True
                pos_hint: {"center_x": .5, "center_y": .5}
                on_press: root.ir_to_dbw_SMG()
